{
  "name": "2004scape-website",
  "version": "1.0.0",
  "type": "module",
  "imports": {
    "#/*": "./src/*"
  },
  "scripts": {
    "dev": "concurrently \"npx tailwindcss -i ./src/css/mod.css -o ./public/css/mod.css --watch\" \"tsx --watch src/app.ts\"",
    "quickstart": "tsx src/app.ts",
    "build": "npx tailwindcss -i ./src/css/mod.css -o ./public/css/mod.css --minify",
    "start": "npm i && npm run build && tsx src/app.ts"
  },
  "dependencies": {
    "@fastify/autoload": "6.0.3",
    "@fastify/cookie": "11.0.2",
    "@fastify/formbody": "8.0.2",
    "@fastify/multipart": "9.0.3",
    "@fastify/rate-limit": "^10.2.2",
    "@fastify/session": "11.1.0",
    "@fastify/static": "8.0.4",
    "@fastify/view": "10.0.2",
    "@tailwindcss/cli": "^4.0.4",
    "bcrypt": "5.1.1",
    "better-sqlite3": "^11.8.1",
    "daisyui": "^5.0.0-beta.7",
    "dayjs": "^1.11.13",
    "dotenv": "16.4.7",
    "ejs": "3.1.10",
    "fastify": "5.2.1",
    "hibp": "^14.1.3",
    "kysely": "0.27.5",
    "lucide": "^0.475.0",
    "mysql2": "3.12.0",
    "obscenity": "^0.4.3",
    "tailwindcss": "^4.0.4"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.2",
    "@types/better-sqlite3": "^7.6.12",
    "@types/ejs": "3.1.5",
    "@types/eslint": "9.6.1",
    "@types/node": "22.10.10",
    "concurrently": "^9.1.2",
    "tsx": "4.19.2",
    "typescript": "5.7.3"
  }
}
